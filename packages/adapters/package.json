{
  "name": "@washi-ui/adapters",
  "version": "1.0.0",
  "description": "Built-in storage adapters for Washi HTML commenting engine",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/washi-ui/washi.git",
    "directory": "packages/adapters"
  },
  "bugs": {
    "url": "https://github.com/washi-ui/washi/issues"
  },
  "homepage": "https://github.com/washi-ui/washi#readme",
  "dependencies": {
    "@washi-ui/core": "workspace:*"
  },
  "keywords": [
    "washi",
    "adapters",
    "localStorage",
    "commenting",
    "annotations"
  ],
  "author": "Marco Chavez",
  "license": "MIT",
  "sideEffects": false
}
